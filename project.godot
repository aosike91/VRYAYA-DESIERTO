; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="VRYAYA DESIERTO"
run/main_scene="res://main_menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

XRToolsUserSettings="*res://addons/godot-xr-tools/user_settings/user_settings.gd"
XRToolsRumbleManager="*res://addons/godot-xr-tools/rumble/rumble_manager.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-xr-tools/plugin.cfg", "res://addons/proton_scatter/plugin.cfg")

[rendering]

textures/vram_compression/import_etc2_astc=true
textures/decals/filter=1
occlusion_culling/occlusion_rays_per_thread=20
occlusion_culling/use_occlusion_culling=true
anti_aliasing/foveation_level="0"

[xr]

openxr/enabled=true
openxr/foveation_level=1
shaders/enabled=true
